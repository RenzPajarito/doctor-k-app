      "target": "esnext",
      "module": "commonjs",
      "jsx": "react-native",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["expo", "react-native"] // Add this line if not present
    }
}
